{
  "name": "retrotrade-firebase",
  "version": "1.0.0",
  "description": "Firebase emulator scripts for RetroTrade project",
  "scripts": {
    "emulator": "firebase emulators:start",
    "emulator:auth": "firebase emulators:start --only auth",
    "emulator:ui": "firebase emulators:start --ui",
    "emulator:debug": "firebase emulators:start --debug",
    "emulator:export": "firebase emulators:export ./emulator-data",
    "emulator:import": "firebase emulators:start --import ./emulator-data"
  },
  "devDependencies": {
    "firebase-tools": "^13.0.0"
  }
}
